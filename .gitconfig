[user]
	name = Kunal Kundu
	email = kunal99kundu@gmail.com
	signingkey  = 287C1397CECC1F35
[github]
	user = tinfoil-knight
[core]
	editor       = vim
	excludesfile = /Users/kunal/.gitignore
[credential]
	helper = osxkeychain
[init]
	defaultBranch = master
[gpg]
	program = gpg
[commit]
	gpgsign = false
	template = /Users/kunal/.stCommitMsg
[pull]
	rebase = true
[push]
	autoSetupRemote = true
[alias]
	# default: is preset for cloned repos, set manually: git remote set-head origin -a
	default = !awk -F "/" '{print $NF}' .git/refs/remotes/origin/HEAD
	s       = status
	d       = diff
	dc      = diff --cached
	ds      = diff --stat
	a       = add
	# au: add only modified and deleted files
	au      = add -u
	aa      = add --all
	c       = commit
	cm      = commit -m
	ca      = commit --amend -m
	can     = commit --amend --no-edit
	p       = push
	po      = push origin
	pom     = push origin master
	pot     = push origin --tags
	pl      = pull
	plo     = pull origin
	plom    = pull origin master
	plod    = !git pull origin $(git default)
  plum    = !git pull upstream $(git default)
	rbd     = !git rebase $(git default)
	# br: -r for remote branches
	br      = branch
	brD     = branch -D
	# co: - for getting back to previous branch
	co      = checkout
	cob     = checkout -b
	cor     = checkout --track
	com     = checkout master
	cod     = !git switch $(git default)
	f       = fetch
	fo      = fetch origin
	rename  = branch -m
	ls      = log --oneline
	last    = log -p -1
	st      = !git add --all && git stash
	stl     = stash list
	sts     = stash show -p
	stp     = stash pop
	sta     = stash apply
	std     = stash drop
	save    = !git add --all && git stash && git stash apply
	alias   = !git config -l | grep alias | cut -c 7-
	cp      = cherry-pick
